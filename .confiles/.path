# default path
PATH=/usr/local/bin/:/usr/bin:/bin:/usr/sbin:/sbin

# Add `~/bin` to the `$PATH`
PATH=$HOME/bin:$PATH

# If there is bin folder in the current location, it takes preference
PATH=./bin:$PATH

# android SDK & NDK
PATH=$PATH:$ANDROID_SDK/bin:$ANDROID_SDK/tools:$ANDROID_SDK/platform-tools:$ANDROID_NDK/bin

export PATH